[user]
	name = vincent poencet
	email = vincepoencet@free.fr
[push]
	default = simple
[core]
	editor = vim
[alias]
	lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	cp = cherry-pick
	st = status
	cl = clone
	c = commit
	cm = commit -m
	ca = commit -a
	cam = commit -a -m 
	co = checkout
	r = reset
	rh = reset --hard HEAD
	sl = stash list
	sa = stash apply
	ss = stash save
